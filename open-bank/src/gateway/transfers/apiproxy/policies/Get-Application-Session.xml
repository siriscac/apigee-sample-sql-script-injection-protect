<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<LookupCache async="false" continueOnError="false" enabled="true" name="Get-Application-Session">
    <DisplayName>Lookup Cache: Application Session</DisplayName>
    <Properties/>
    <CacheResource>application-session-cache</CacheResource>
    <AssignTo>session.Payload</AssignTo>
    <Scope>Global</Scope>
    <CacheKey>
        <KeyFragment ref="auth.transaction.session.id"></KeyFragment>
    </CacheKey>
</LookupCache>
