<ExtractVariables name="Extract.RequestLocationParams">
    <DisplayName>Extract.RequestLocationParams</DisplayName>
    <Source>request</Source>
    <QueryParam name="latitude">
        <Pattern ignoreCase="true">{latitude}</Pattern>
    </QueryParam>
    <QueryParam name="longitude">
        <Pattern ignoreCase="true">{longitude}</Pattern>
    </QueryParam>
    <QueryParam name="radius">
        <Pattern ignoreCase="true">{radius}</Pattern>
    </QueryParam>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</ExtractVariables>