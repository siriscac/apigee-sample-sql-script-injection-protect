<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage name="set-success-token-response">
  <DisplayName>Assign: SMS token response</DisplayName>
	<Set> 
		<Headers>
                <Header name="Access-Control-Allow-Origin">{request.header.origin}</Header>
                <Header name="Access-Control-Allow-Credentials">true</Header>
                <Header name="Access-Control-Allow-Methods">OPTIONS,GET,POST,PUT,DELETE,HEAD</Header>
                <Header name="Access-Control-Allow-Headers">Authorization,Content-Type,Accept,Origin,x-apikey</Header>
                <Header name="Access-Control-Max-Age">86400</Header>
            </Headers>
	    <Payload/> 
		<StatusCode>201</StatusCode>
	</Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="true" transport="http" type="response"/>
</AssignMessage>
