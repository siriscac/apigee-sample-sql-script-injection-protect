<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables async="false" continueOnError="false" enabled="true" name="Extract-Identifier">
    <DisplayName>Extract: Identifier</DisplayName>
    <Properties/>
    <Variable name="proxy.pathsuffix">
    	<Pattern>/otp/{identifier}</Pattern>
    </Variable>
    <Variable name="proxy.pathsuffix">
      <Pattern>/otp/{identifier}/{token}</Pattern>
    </Variable>
    <Variable name="proxy.pathsuffix">
        <Pattern>/confirmation/{identifier}</Pattern>
    </Variable>
	<IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</ExtractVariables>
