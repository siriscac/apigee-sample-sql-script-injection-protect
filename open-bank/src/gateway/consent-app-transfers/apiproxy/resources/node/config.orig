{
  "base_path": "/internal/transfer-consent",
  "logo_uri": "http://dev-dbank.devportal.apigee.com/sites/default/files/logo4.png",
  "styles": {
    "header_background_color": "#FFF"
  },
  "consentTransaction": {
    "transactionEndpoint": "https://__HOST__/internal/apis/v1/session/transactions/",
    "method": "GET",
    "headers": {
      "x-apikey": "__APIINTKEY__"
    }
  },
  "authenticationTransaction": {
    "transactionEndpoint": "https://__HOST__/internal/connector/v1/authenticate",
    "method": "POST",
    "headers": {
      "x-apikey": "__APIINTKEY__"
    }
  },
  "accessTokenTransaction": {
    "transactionEndpoint": "https://__HOST__/apis/v1/transfers/authorized/transactions/",
    "method": "POST",
    "headers": {
      "x-apikey": "__APIINTKEY__"
    }
  },
  "transferTransaction": {
    "transactionEndpoint": "https://__HOST__/apis/v1/transfers/complete/",
    "method": "POST",
    "headers": {
      "Authorization": "Bearer "
    }
  },
  "generateOtp": {
    "transactionEndpoint": "https://__HOST__/internal/apis/v1/sms/otp/",
    "method": "POST",
    "headers": {
      "x-apikey": "__APIINTKEY__"
    }
  },
  "validateOtp": {
    "transactionEndpoint": "https://__HOST__/internal/apis/v1/sms/otp/",
    "method": "POST",
    "headers": {
      "x-apikey": "__APIINTKEY__"
    }
  },
  "sendConfirmationSMS": {
    "transactionEndpoint": "https://__HOST__/internal/apis/v1/sms/confirmation/",
    "method": "POST",
    "headers": {
      "x-apikey": "__APIINTKEY__"
    }
  },
  "getAccounts": {
    "transactionEndpoint": "https://__HOST__/internal/connector/v1/accounts",
    "method": "GET",
    "headers": null
  },
  "errors": {
    "user_cancel": {
      "code": "Consent_Rejected",
      "description": "User rejected consent"
    }
  },
  "loa": {
    "1": {
      "steps": [
        {
          "name": "OTP",
          "weight": 0
        }
      ]
    },
    "2": {
      "steps": [
        {
          "name": "UID_PWD",
          "weight": 0
        }
      ]
    },
    "3": {
      "steps": [
        {
          "name": "UID_PWD",
          "weight": 0
        },
        {
          "name": "OTP",
          "weight": 1
        }
      ]
    }
  }
}
