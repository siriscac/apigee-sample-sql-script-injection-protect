<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="Fault.InvalidTransaction">
    <DisplayName>Fault.InvalidTransaction</DisplayName>
    <Properties/>
    <FaultResponse>
        <Set>
            <Headers/>
            <Payload contentType="application/json" contentPrefix="@" contentSuffix="#">
                {
                    "error": "Unauthorized Transaction",
                    "description": "Access Token does not contain a Customer Id or Account Id reference"
                }
            </Payload>
            <StatusCode>401</StatusCode>
            <ReasonPhrase>Unauthorized</ReasonPhrase>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>