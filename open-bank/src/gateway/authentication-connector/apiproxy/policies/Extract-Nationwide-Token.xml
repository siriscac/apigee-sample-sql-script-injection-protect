<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables async="false" continueOnError="false" enabled="true" name="Extract-Nationwide-Token">
    <DisplayName>Extract: User Access Token</DisplayName>
    <Properties/>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <JSONPayload>
        <Variable name="baas_accesstoken">
            <JSONPath>$.access_token</JSONPath>
        </Variable>
         <Variable name="customerNumber">
            <JSONPath>$.user.username</JSONPath>
        </Variable>
    </JSONPayload>
    <Source clearPayload="false">response</Source>
</ExtractVariables>
