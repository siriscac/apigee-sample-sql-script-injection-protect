<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Cache name="sms-token-cache">
    <ExpirySettings>
        <TimeoutInSec>600</TimeoutInSec>
    </ExpirySettings>
    <Compression>
        <MinimumSizeInKB>1024</MinimumSizeInKB>
    </Compression>
    <Description>cache for SMS OTP</Description>
    <DiskSizeInMB>0</DiskSizeInMB>
    <InMemorySizeInKB>8000</InMemorySizeInKB>
    <MaxElementsInMemory>3000000</MaxElementsInMemory>
    <MaxElementsOnDisk>1000</MaxElementsOnDisk>
    <OverflowToDisk>true</OverflowToDisk>
    <Persistent>false</Persistent>
</Cache>
